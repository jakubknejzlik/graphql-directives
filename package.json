{
  "name": "gql-directives",
  "version": "0.0.3",
  "description": "Collection of GraphQLDirectives",
  "author": "jakub.knejzlik",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/jakubknejzlik/ts-thunk.git"
  },
  "scripts": {
    "test": "npm run test:unit && npm run test:cov",
    "test:unit": "jest",
    "test:cov": "jest --coverage"
  },
  "dependencies": {
    "js-sha512": "^0.8.0"
  },
  "peerDependencies": {
    "graphql": "*"
  },
  "devDependencies": {
    "graphql": "^14.0.2",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}
